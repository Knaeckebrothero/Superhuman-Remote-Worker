{
  "agent": {
    "model": "gpt-oss-120b",
    "temperature": 0.0,
    "max_iterations": 100,
    "reasoning_level": "high"
  },
  "chain": {
    "model": "gpt-oss-120b",
    "temperature": 0.2,
    "reasoning_level": "medium"
  },
  "document_ingestion": {
    "model": "gpt-oss-120b",
    "temperature": 0.0,
    "reasoning_level": "medium",
    "chunking_strategy": "legal",
    "max_chunk_size": 1000,
    "chunk_overlap": 200,
    "extraction_mode": "balanced",
    "min_confidence": 0.6,
    "parallel_extraction": true,
    "max_candidates": 1000,
    "duplicate_similarity_threshold": 0.95,
    "require_entity_existence": false,
    "auto_integrate": false
  },
  "creator_agent": {
    "model": "gpt-oss-120b",
    "temperature": 0.0,
    "reasoning_level": "high",
    "research_depth": "thorough",
    "min_confidence_threshold": 0.6,
    "chunk_strategy": "legal",
    "max_chunk_size": 1000,
    "polling_interval_seconds": 30,
